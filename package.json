{
	"name": "errorhandler",
	"version": "1.0.0",
	"private": false,
	"devDependencies": {
		"eslint": "^6.8.0",
		"@ui5/cli": "2.2.2",
		"eslint-config-airbnb": "^18.1.0",
		"eslint-config-airbnb-base": "^14.1.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-import": "^2.20.1",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-react": "^7.19.0",
		"eslint-plugin-react-hooks": "^2.5.0",
		"prettier": "^2.0.5",
		"rimraf": "^3.0.2"
	},
	"scripts": {
		"clean": "rimraf dist",
		"build": "rimraf dist && ui5 build --include-task=generateManifestBundle generateCachebusterInfo ",
		"build-for-deploy": "npm run build && npm run flatten && npm run clean-after-flatten",
		"flatten": "cp -r dist/resources/errorhandler/library/* dist && cp dist/resources/errorhandler/library/.library dist && cp dist/resources/.Ui5RepositoryTextFiles dist",
		"clean-after-flatten": "rimraf dist/resources dist/test-resources"
	},
	"license": "UNLICENSED"
}