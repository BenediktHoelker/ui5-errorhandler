{
    "name": "@iot/ui5-errorhandler",
    "description": "Handle UI5- and OData-messages and display them in MessagePopover/-Box",
    "version": "2.2.7",
    "private": false,
    "devDependencies": {
        "@sap/ux-ui5-tooling": "1",
        "@ui5/cli": "^2.11.1",
        "@ui5/fs": "^2.0.6",
        "@ui5/logger": "^2.0.1"
    },
    "dependencies": {
        "ui5-task-flatten-library": "^0.2.2",
        "ui5-task-zipper": "^0.4.4"
    },
    "ui5": {
        "dependencies": [
            "ui5-task-flatten-library",
            "ui5-task-zipper"
        ]
    },
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "scripts": {
        "lint": "eslint src",
        "build": "ui5 build --clean-dest --include-task=generateManifestBundle generateCachebusterInfo",
        "build:cf": "npm run build",
        "deploy:cf": "cf deploy mta_archives/iot-errorhandler_0.0.1.mtar",
        "build-and-deploy:cf": "mbt build && npm run deploy:cf"
    },
    "license": "UNLICENSED"
}